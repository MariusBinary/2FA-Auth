<Application x:Class="_2FA_Auth.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:_2FA_Auth"
                         StartupUri="UI/Pages/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <!--Values-->
                <ResourceDictionary Source="pack://application:,,,/Resources/Values.xaml"/>
                <!--Theme-->
                <ResourceDictionary Source="pack://application:,,,/Resources/Theme.xaml"/>
                <!--Assets-->
                <ResourceDictionary Source="pack://application:,,,/Resources/Assets.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
